# Server Configuration
PORT=3000
NODE_ENV=development

# MongoDB Configuration
MONGODB_URI=mongodb://localhost:27017/licener
# For MongoDB Atlas: mongodb+srv://<username>:<password>@cluster0.mongodb.net/licener

# Session and Security
SECRET_KEY=your_secret_key_here
JWT_SECRET=your_jwt_secret_here
SESSION_LIFETIME=3600000

# Mail Configuration
MAIL_HOST=smtp.example.com
MAIL_PORT=587
MAIL_USER=your_email@example.com
MAIL_PASS=your_email_password
MAIL_FROM=noreply@example.com

# Admin User Setup
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=admin_password

# Web Search APIs
SERPER_API_KEY=your_serper_api_key_here
USE_REAL_SEARCH=false  # Set to true to use the real Serper.dev API 